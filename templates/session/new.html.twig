{% extends 'base.html.twig' %}

{% block title %}Ajouter une session{% endblock %}

{% block body %}

<h4>Ajouter une session</h4>

{{ form_start(formAddSession) }}
<div class="row">
    <div class="col-sm">
        {{ form_row(formAddSession.titre) }}
    </div>
    <div class="col-sm">
        {{ form_row(formAddSession.nbrPlaces) }}
    </div>
</div>
<div class="row">
    <div class="col-sm">
        {{ form_row(formAddSession.dateDebut) }}
        {{ form_row(formAddSession.dateFin) }}
    </div>
    <div class="col-sm">
        {{ form_row(formAddSession.stagiaires) }}
        {{ form_row(formAddSession.formateur) }}
    </div>
</div>
{{ form_row(formAddSession.Valider) }}
{{ form_end(formAddSession) }}

{% endblock %}


